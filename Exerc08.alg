algoritmo "intervalo"

var
  cont, fat, num: inteiro
inicio
         fat <- 1

         escreva("informe o numero ")
         leia(num)

         para cont de 1 ate num faca
              fat <- fat * cont
         fimpara

         escreval("")
         escreval("O fatorial de[",num," ] e: ",fat)
                
       
fimalgoritmo
