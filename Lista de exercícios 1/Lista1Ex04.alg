algoritmo "semnome"

var
   num : inteiro

inicio
   escreva("Digite um numero inteiro: ")
   leia(num)

   se((num > 100) e (num < 200))entao
        escreva("o numero digitado esta dentro do intervalo!")
   senao
        escreva("o numero digitado esta fora do intervalo!")
   fimse
fimalgoritmo

